{
    "name": "@shared/utils",
    "version": "0.1.0",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "dependencies": {
        "@aws-sdk/client-cloudwatch": "^3.673.0",
        "@aws-sdk/client-cloudwatch-logs": "^3.673.0"
    },
    "devDependencies": {
        "@types/jest": "^29.5.12",
        "jest": "^29.7.0",
        "ts-jest": "^29.1.2"
    },
    "scripts": {
        "build": "tsc -p tsconfig.json",
        "test": "jest",
        "test:watch": "jest --watch"
    },
    "jest": {
        "preset": "ts-jest",
        "testEnvironment": "node",
        "testMatch": [
            "**/__tests__/**/*.test.ts"
        ],
        "collectCoverageFrom": [
            "src/**/*.ts",
            "!src/**/*.d.ts"
        ]
    }
}